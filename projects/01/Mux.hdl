/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    Nand(a=a, b=nsel, out=n1);
    Nand(a=sel, b=b, out=n2);
    Nand(a=n1, b=n2, out=out);
}
